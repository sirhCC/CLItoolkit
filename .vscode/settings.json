{
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "typescript.preferences.includePackageJsonAutoImports": "auto",
    "files.exclude": {
        "**/node_modules": true,
        "**/dist": true,
        "**/coverage": true,
        "**/temp-files": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/dist": true,
        "**/coverage": true,
        "**/temp-files": true
    }
}